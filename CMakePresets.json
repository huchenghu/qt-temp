{
  "version": 8,
  "cmakeMinimumRequired": {
    "major": 3,
    "minor": 28,
    "patch": 0
  },
  "include": [],
  "configurePresets": [
    {
      "name": "default",
      "hidden": true,
      "inherits": [
        "toolchain-qt6",
        "generator-ninja",
        "compiler-clang",
        "link-dynamic"
      ],
      "binaryDir": "${sourceDir}/build/${presetName}",
      "installDir": "${sourceDir}/install/${presetName}",
      "cacheVariables": {
        "CMAKE_EXPORT_COMPILE_COMMANDS": true,
        "CMAKE_COMPILE_WARNING_AS_ERROR": true
      },
      "debug": {
        "find": false,
        "output": false,
        "tryCompile": false
      },
      "warnings": {
        "deprecated": true,
        "dev": false,
        "systemVars": true,
        "uninitialized": true,
        "unusedCli": true
      },
      "errors": {
        "deprecated": false,
        "dev": false
      },
      "environment": {}
    },
    {
      "name": "toolchain-vcpkg",
      "hidden": true,
      "toolchainFile": "$env{VCPKG_CUSTOM}"
    },
    {
      "name": "toolchain-qt6",
      "hidden": true,
      "toolchainFile": "$env{QT6_CUSTOM}",
      "cacheVariables": {
        "USE_QT6": true
      }
    },
    {
      "name": "toolchain-qt5",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_PREFIX_PATH": "$env{QT5_CUSTOM}",
        "USE_QT5": true
      }
    },
    {
      "name": "platform-darwin",
      "hidden": true,
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Darwin"
      },
      "cacheVariables": {
        "CMAKE_APPLE_SILICON_PROCESSOR": "arm64"
      }
    },
    {
      "name": "platform-linux",
      "hidden": true,
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Linux"
      }
    },
    {
      "name": "platform-windows",
      "hidden": true,
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Windows"
      }
    },
    {
      "name": "generator-unix-makefiles",
      "hidden": true,
      "generator": "Unix Makefiles"
    },
    {
      "name": "generator-ninja",
      "hidden": true,
      "generator": "Ninja"
    },
    {
      "name": "generator-ninja-multi-config",
      "hidden": true,
      "generator": "Ninja Multi-Config"
    },
    {
      "name": "generator-visual-studio",
      "hidden": true,
      "generator": "Visual Studio 17 2022"
    },
    {
      "name": "compiler-clang",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_C_COMPILER": "clang",
        "CMAKE_CXX_COMPILER": "clang++"
      }
    },
    {
      "name": "compiler-gcc",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_C_COMPILER": "gcc",
        "CMAKE_CXX_COMPILER": "g++"
      }
    },
    {
      "name": "compiler-msvc",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_C_COMPILER": "cl",
        "CMAKE_CXX_COMPILER": "cl"
      },
      "architecture": {
        "value": "x64",
        "strategy": "external"
      },
      "toolset": {
        "value": "host=x64",
        "strategy": "external"
      }
    },
    {
      "name": "link-dynamic",
      "hidden": true,
      "cacheVariables": {
        "BUILD_SHARED_LIBS": true
      }
    },
    {
      "name": "link-static",
      "hidden": true,
      "cacheVariables": {
        "BUILD_SHARED_LIBS": false
      }
    },
    {
      "name": "build-debug",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "name": "build-release",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "name": "build-relwithdebinfo",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "RelWithDebInfo"
      }
    },
    {
      "name": "build-minsizerel",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "MinSizeRel"
      }
    },
    {
      "name": "debug",
      "inherits": [
        "default",
        "build-debug"
      ]
    },
    {
      "name": "release",
      "inherits": [
        "default",
        "build-release"
      ]
    },
    {
      "name": "relwithdebinfo",
      "inherits": [
        "default",
        "build-relwithdebinfo"
      ]
    }
  ],
  "buildPresets": [
    {
      "name": "debug",
      "configurePreset": "debug",
      "configuration": "Debug",
      "cleanFirst": true
    },
    {
      "name": "release",
      "configurePreset": "release",
      "configuration": "Release",
      "cleanFirst": true
    },
    {
      "name": "relwithdebinfo",
      "configurePreset": "relwithdebinfo",
      "configuration": "RelWithDebInfo",
      "cleanFirst": true
    }
  ],
  "testPresets": [
    {
      "name": "debug",
      "configurePreset": "debug"
    },
    {
      "name": "release",
      "configurePreset": "release"
    },
    {
      "name": "relwithdebinfo",
      "configurePreset": "relwithdebinfo"
    }
  ],
  "packagePresets": [
    {
      "name": "debug",
      "configurePreset": "debug"
    },
    {
      "name": "release",
      "configurePreset": "release"
    },
    {
      "name": "relwithdebinfo",
      "configurePreset": "relwithdebinfo"
    }
  ],
  "workflowPresets": [
    {
      "name": "debug",
      "steps": [
        {
          "type": "configure",
          "name": "debug"
        },
        {
          "type": "build",
          "name": "debug"
        },
        {
          "type": "test",
          "name": "debug"
        },
        {
          "type": "package",
          "name": "debug"
        }
      ]
    },
    {
      "name": "release",
      "steps": [
        {
          "type": "configure",
          "name": "release"
        },
        {
          "type": "build",
          "name": "release"
        },
        {
          "type": "test",
          "name": "release"
        },
        {
          "type": "package",
          "name": "release"
        }
      ]
    },
    {
      "name": "relwithdebinfo",
      "steps": [
        {
          "type": "configure",
          "name": "relwithdebinfo"
        },
        {
          "type": "build",
          "name": "relwithdebinfo"
        },
        {
          "type": "test",
          "name": "relwithdebinfo"
        },
        {
          "type": "package",
          "name": "relwithdebinfo"
        }
      ]
    }
  ]
}
